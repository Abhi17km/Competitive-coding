class Solution {
    public int firstUniqChar(String s) {
        int count=0;int ind=-1;
            for(int j=0;j<s.length();j++)
                if(s.indexOf(s.charAt(j))==(s.lastIndexOf(s.charAt(j))))
                    return s.indexOf(s.charAt(j)) ;
                return -1;
         
        
                    
    }   
}
